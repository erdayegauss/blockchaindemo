{
  "engines": {
    "composer": ""
  },
  "name": "bond-network",
  "version": "0.1.0",
  "description": "Bond Reference Data Sharing Business Network",
  "scripts": {
    "start": "start-network"
  },
  "keywords": [
    "bonds",
    "reference data",
    "finance",
    "composer",
    "composer-network"
  ],
  "author": "Hyperledger Composer",
  "license": "Apache-2.0",
  "devDependencies": {
    "chai": "^3.5.0",
    "composer-admin": "0.19.13",
    "composer-cli": "0.19.13",
    "composer-client": "0.19.13",
    "composer-common": "0.19.13",
    "composer-connector-embedded": "0.19.13",
    "eslint": "^3.6.1",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.5.5",
    "license-check-and-add": "~2.3.0",
    "mkdirp": "^0.5.1",
    "mocha": "^3.2.0",
    "moment": "^2.17.1"
  },
  "license-check-and-add-config": {
    "folder": ".",
    "license": "LICENSE.txt",
    "exact_paths_method": "EXCLUDE",
    "exact_paths": [
      "composer-logs",
      "dist",
      "node_modules",
      "out",
      ".git"
    ],
    "file_type_method": "EXCLUDE",
    "file_types": [
      ".yml"
    ],
    "insert_license": false,
    "license_formats": {
      "js|cto|acl": {
        "prepend": "/*",
        "append": " */",
        "eachLine": {
          "prepend": " * "
        }
      },
      "md": {
        "file": "../../markdown-license.txt"
      }
    }
  },
  "dependencies": {
    "composer-common": "0.19.13",
    "composer-runtime-hlfv1": "0.19.13"
  }
}
